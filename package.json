{
  "name": "solito-blank",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^18.2.21",
    "eslint": "^8.21.0",
    "react-native": "0.74.5",
    "turbo": "^1.4.2",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "web": "cd apps/next && yarn next",
    "db:migrate": "cd packages/app/db && doppler run -- npx drizzle-kit generate --config ./db/drizzle.config.ts"
  },
  "packageManager": "yarn@4.1.0",
  "resolutions": {
    "typescript": "5.6.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint-plugin-drizzle": "^0.2.3"
  }
}
